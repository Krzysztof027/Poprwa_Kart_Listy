<Window x:Class="kart_Listy.Zapisdopliku"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:kart_Listy"
        mc:Ignorable="d"
        Title="Zapisdopliku" Height="450" Width="800">
    <StackPanel>
        <Menu>
            <MenuItem Header="Plik"></MenuItem>
            <MenuItem Header="Okno">
            <MenuItem Header="Zamknij" Click="Click_Zamknij"></MenuItem>
            <MenuItem Header="Nowe okno modalne " Click="Click_Nowe_Modalne"/>
            <MenuItem Header="Nowe okno niemodalne" Click="Click_Nowe_Niemodalne"/>
            </MenuItem>
        </Menu>
    </StackPanel>
</Window>
